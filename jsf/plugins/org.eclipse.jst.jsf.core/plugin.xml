  </extension>
  <extension
        point="org.eclipse.jst.jsf.core.customViewMapper">
     <customViewMapper
           class="org.eclipse.jst.jsf.designtime.internal.view.mapping.mappers.ValueHolderAttributeMapper"
           id="valueHolderAttributeMapper">
     </customViewMapper>
  </extension>
  
</plugin>
